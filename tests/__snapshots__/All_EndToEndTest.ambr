          cls=NaturalOrderGrouper,
          help=__doc__,
          no_args_is_help=True,
          pretty_exceptions_show_locals=False,
          pretty_exceptions_enable=False,
      )
      
      
      # ----------------------------------------------------------------------
      @app.command("Add")
      def Add(
          x: int,
          y: int,
      ) -> None:
          """Adds 2 values."""
      
          sys.stdout.write(str(Math.Add(x, y)))
      
      
      # ----------------------------------------------------------------------
      @app.command("Sub")
      def Sub(
          x: int,
          y: int,
      ) -> None:
          """Subtracts 2 values."""
      
          sys.stdout.write(str(Math.Sub(x, y)))
      
      
