      import sys
      
      from pathlib import Path
      
      
      # Parse the arguments
      is_debug = False
      is_force = False
      is_verbose = False
      is_package = False
      no_cache = False
      
      display_flags: list[str] = []
      
      # First arg is the script name, second arg is the name of the shell script to write to
      for arg in sys.argv[2:]:
          if arg == "--debug":
              is_debug = True
          elif arg == "--force":
              is_force = True
          elif arg == "--verbose":
              is_verbose = True
          elif arg == "--package":
              is_package = True
              display_flags.append("package")
          elif arg == "--no-cache":
              no_cache = True
          else:
